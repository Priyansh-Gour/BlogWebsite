<%- include("parshals/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post" autocomplete="off">
    <div class="form-group">
        <label for="text">Title</label>
        <input id="text" type="text" class="form-control" name="postTitle">
    </div>
    <div class="form-group">
        <label for="Textarea">Post</label>
        <textarea id="Textarea" name="postBody" cols="40" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include("parshals/footer"); -%>
